{
    "name": "olcs/olcs-utils",
    "description": "OLCS Utils",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@repo.shd.ci.nonprod.dvsa.aws:olcs/olcs-logging.git"
        },
        {
            "packagist": false
        },
        {
            "url": "http://devrepo.shd.ci.nonprod.dvsa.aws",
            "type": "composer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "zendframework/zendframework": "~2.3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0.0",
        "mockery/mockery": "~0.9.3",
        "olcs/olcs-logging": "dev-release/4.13"
    },
    "config": {
        "preferred-install": "dist",
        "secure-http" : false
    },
    "autoload": {
        "psr-4": {
            "Dvsa\\Olcs\\Utils\\": "src/"
        }
    }
}
